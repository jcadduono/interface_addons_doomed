<Bindings>
	<Binding name="Toggle Targets +" header="DOOMED" category="ADDONS">
		Doomed_ToggleTargetMode()
	</Binding>
	<Binding name="Toggle Targets -" header="DOOMED" category="ADDONS">
		Doomed_ToggleTargetModeReverse()
	</Binding>
	<Binding name="Set Targets to 1" header="DOOMED" category="ADDONS">
		Doomed_SetTargetMode(1)
	</Binding>
	<Binding name="Set Targets to 2+" header="DOOMED" category="ADDONS">
		Doomed_SetTargetMode(2)
	</Binding>
	<Binding name="Set Targets to 3+" header="DOOMED" category="ADDONS">
		Doomed_SetTargetMode(3)
	</Binding>
	<Binding name="Set Targets to 5+" header="DOOMED" category="ADDONS">
		Doomed_SetTargetMode(4)
	</Binding>
</Bindings>